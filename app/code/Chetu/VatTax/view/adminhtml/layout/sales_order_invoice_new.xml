<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="invoice_totals">
            <action method="setTemplate">
                <argument name="template" xsi:type="string">Chetu_VatTax::order/totals_invoice_new.phtml</argument>
            </action>
        </referenceBlock>
        <referenceBlock name="order_items">
            <block class="Magento\Weee\Block\Adminhtml\Items\Price\Renderer" name="column_total" template="Chetu_VatTax::items/price/total.phtml" group="column"/>
        </referenceBlock>
    </body>
</page>
